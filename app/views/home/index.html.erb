<h1>Welcome to Roster</h1>	
<% if user_signed_in? %>
		<h2><%= link_to 'Sign Out',destroy_user_session_path, method: :delete %></h2>
		<h2>Add your shift Details <%= link_to 'here',new_shift_detail_path%></h2>
<% else %>
		<h2><%= link_to 'Log in', new_user_session_path,method: :get%></h2>
		<h2><%= link_to 'Sign Up', new_user_registration_path,method: :get%></h2>
<% end %>